// vanillaSum for []float{{.bits}}
func (F{{.bits}}) vanillaSum(a []float{{.bits}})float{{.bits}} {
    acc:=float{{.bits}}(0)
    for _,v:=range a{
        acc+=v
    }
    return acc
}

